<Defs>

  <!-- =========================== Gold Dragon =========================== -->

  <ThingDef ParentName="RDragonRaceBase">
    <defName>Gold_Dragon</defName>
    <label>gold dragon</label>
    <description>Gold dragons are highly prized due to their golden scales and friendly disposition. Many struggling raiders and tribesmen seek out these generous beasts to ask for a part of their hoard, however many are turned down for their greed and hostility.</description>
    <tools>
      <li>
        <label>left claw</label>
        <capacities>
          <li>Cut</li>
        </capacities>
        <power>20</power>
        <armorPenetration>0.2</armorPenetration>
        <cooldownTime>1.5</cooldownTime>
        <linkedBodyPartsGroup>FrontLeftPaw</linkedBodyPartsGroup>
      </li>

      <li>
        <label>right claw</label>
        <capacities>
          <li>Cut</li>
        </capacities>
        <power>20</power>
        <armorPenetration>0.2</armorPenetration>
        <cooldownTime>1.5</cooldownTime>
        <linkedBodyPartsGroup>FrontRightPaw</linkedBodyPartsGroup>
      </li>

      <li>
        <label>pyro bite</label>
        <capacities>
          <li>Pyro_Bite</li>
        </capacities>
        <power>25</power>
        <armorPenetration>0.9</armorPenetration>
        <cooldownTime>6.0</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
        <chanceFactor>0.01</chanceFactor>
      </li>

      <li>
        <label>tail whip</label>
        <capacities>
          <li>Scratch</li>
          <li>Cut</li>
        </capacities>
        <power>5</power>
        <armorPenetration>.4</armorPenetration>
        <cooldownTime>5.0</cooldownTime>
        <linkedBodyPartsGroup>Tail</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>20</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>

      <li>
        <label>horn</label>
        <capacities>
          <li>Scratch</li>
          <li>Stab</li>
        </capacities>
        <power>40</power>
        <armorPenetration>1.30</armorPenetration>
        <cooldownTime>5.0</cooldownTime>
        <linkedBodyPartsGroup>Horns</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>30</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
    </tools>
    <comps>
      <li Class="DD.CompProperties_CrossbredEggLayer">
        <eggUnfertilizedDef>EggDragonUnfertilized</eggUnfertilizedDef>
        <eggFertilizedDef>EggGoldDragonFertilized</eggFertilizedDef>
        <eggFertilizationCountMax>1</eggFertilizationCountMax>
        <eggLayIntervalDays>40</eggLayIntervalDays>
        <eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
        <eggCountRange>2</eggCountRange>
		<failChance>0.25</failChance>
		<paternalChance>0.5</paternalChance>
		<maternalChance>0.5</maternalChance>
      </li>
    </comps>
    <race>
      <wildness>0.96</wildness>
      <useMeatFrom>Red_Dragon</useMeatFrom>
      <wildBiomes>
        <TropicalRainforest>0.001</TropicalRainforest>
        <AridShrubland>0.002</AridShrubland>
        <Desert>0.002</Desert>
        <ExtremeDesert>0.003</ExtremeDesert>
      </wildBiomes>
    </race>
  </ThingDef>

  <PawnKindDef ParentName="AnimalKindBase">
